. path.sh

fstprint data/local/lang/L.fst  ~/w2019a/learn-fst/Ltext.fst
fstprint data/local/lang_test/G.fst ~/w2019a/learn-fst/Gtext.fst

#using  sorted

fstinfo data/local/lang/L.fst   // get result display:Sort by output label and unsort by input label 
#in order to compare 

fstarcsort --sort_type=ilabel data/local/lang/L.fst ~/w2019a/learn-fst/Linput_sorted.fst  // get result display:Sort by iutput label and and unsort by output label

# Creates the composed FST.
fstcompose  data/local/lang/L.fst  ~/w2019a/learn-fst/Linput_sorted.fst ~/w2019a/learn-fst/comp.fst
# Just keeps the output label 
fstproject --project_output ~/w2019a/learn-fst/comp.fst ~/w2019a/learn-fst/result.fst
